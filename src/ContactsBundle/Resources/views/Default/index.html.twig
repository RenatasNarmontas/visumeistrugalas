{% extends '::base.html.twig' %}
{% form_theme form 'bootstrap_3_horizontal_layout.html.twig' %}

{% block title %}Contacts{% endblock%}

{% block body %}
    <header>
        <h1>Contacts</h1>
    </header>
        <form action="" method="post" {{ form_enctype(form) }}>
            {{ form_errors(form) }}

            {{ form_row(form.email) }}
            {{ form_row(form.message) }}

            {{ form_rest(form) }}

            <input type="submit" value="Submit" />
        </form>
{% endblock %}
