{% block body -%}
<h1>Demo creation</h1>

<div class="form_error"></div>
<form method="POST" class="ajaxForm" action="{{path('users_disable')}}" {{ form_enctype(form) }}>
    <div id="form_body">
        {% include 'AppBundle:Admin:form.html.twig' with {'form': form} %}
    </div>

    <button type="submit" class="btn btn-primary">Submit</button>
    {{form_rest(form)}}
</form>

<ul class="record_actions">
    <li>
        Back to the list
    </li>
</ul>
{% endblock %}


{% block javascripts %}
    <script>
        initAjaxForm();
    </script>
{% endblock %}